{
  "presets": ["flow", "react", ["es2015"], "es2016", "stage-2"],
  "env": {
    "development": {
      "plugins": [
        "styled-components",
        "transform-object-rest-spread",
        "transform-class-properties",
        "babel-plugin-styled-components"
      ]
    },
    "production": {
      "plugins": [
        "styled-components",
        "transform-object-rest-spread",
        "transform-class-properties",
        "babel-plugin-styled-components"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "babel-plugin-styled-components"
      ]
    },
    "server": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
